<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.huateng.xhcp.mapper.system.UserLoginHistMapper">
	<!-- 登录历史信息 -->
	<insert id="addUserLoginHist" parameterType="UserLoginHist" useGeneratedKeys="true" keyProperty="login_id">
		INSERT INTO tbl_user_login_hist (account_id,account_name,login_info,login_time)
		VALUES(#{account_id,jdbcType=VARCHAR},#{account_name,jdbcType=VARCHAR},#{login_info,jdbcType=VARCHAR},date_format(now(),'%Y%m%d%H%i%s'))
	</insert>
</mapper>
